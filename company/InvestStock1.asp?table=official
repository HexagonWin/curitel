function clear_left_block()
{
	var obj;
	for (var i=1; i<5; i++)
	{ 									
		obj = MM_findObj("left0"+i);
		obj.style.display = "none";
	}
}

var old_left_block="block";
function left_block_shake(block)
{
	var obj, obj2;
	obj = MM_findObj(block);
	if ( old_left_block != block)
	{
		if (obj.style.display == "none")
		{

			clear_left_block();
			obj.style.display = "";
			old_left_block=block;
		}
		else
		{
			clear_left_block();
			obj.style.display="none";
			old_left_block=block;
		}
	}
	else
	{
		obj.style.display="none";
		old_left_block="shake";
	}
}




