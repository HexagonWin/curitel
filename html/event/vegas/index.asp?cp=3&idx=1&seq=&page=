} 
function Timer1() 
{ 
	sec=sec-1;
	if(sec == -1) 
	{ 
		sec = 59; 
		min = min-1; 
	} 
	if(min == -1)
	{                                             
		min=59; 
		hour = hour - 1; 
	} 
	if(hour == -1)
	{                                             
		hour = 23;
		day = day - 1;
	} 
	if(sec == 0 && min == 0 && hour == 0 ) return; 
	document.all.curitimer.innerHTML =  tstr(hour) + ':' + tstr(min) + ':' + tstr(sec); 
	window.setTimeout('Timer1()',1000); 
}
function tstr(str)
{
	if(str<10) return "0" + str;
	return str;
}




