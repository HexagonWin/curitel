<script language='javascript'>
function lastacademy(sele)
{
     var afrm = document.academyfrm
     var sfrm = document.scatefrm     
     location.href="festival_total.asp?scategory=" + sfrm.scategory.options[sfrm.scategory.selectedIndex].value + "&current_aidx=" + afrm.academy_list.options[afrm.academy_list.selectedIndex].value;
}
function changeorder(sele)
{
     var afrm = document.academyfrm
     var sfrm = document.scatefrm
     location.href="festival_total.asp?scategory=" + sfrm.scategory.options[sfrm.scategory.selectedIndex].value + "&current_aidx=" + afrm.academy_list.options[afrm.academy_list.selectedIndex].value;
}
</script>
