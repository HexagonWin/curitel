<script language='javascript'>
function _del(idx,uid)
{
     if(uid == "")
     {
          if(confirm("삭제 하시겠습니까? 삭제 하시면 복구가 불가능 합니다."))
          {
               location.href="http://file.curitel.com/html/community/movielife/socool/apply_save.asp?idx=" + idx + "&act=delete"
          }
     }
     else
     {
          alert("작성자만 삭제 할 수 있습니다.");
     }
}
function point_send(frm)
{
     if(!frm.point[0].checked && !frm.point[1].checked && !frm.point[2].checked && !frm.point[3].checked && !frm.point[4].checked)
     {
          alert("점수를 선택해주세요");
          frm.point[0].focus();
          return false;
     }
     
     return true;
}
</script>
