<script language="javascript">
function loadData(sel,target) {

	var trigger = sel.options[sel.selectedIndex].value;

	var form = sel.form.name;

	dynamic.src = "./gugun.asp?form=" + form + "&trigger=" + trigger + "&target=" + target;
}
function search(frm)
{
     if(frm.sido.selectedIndex == 0)
     {
          alert("시/도 선택을 해주세요");
          frm.sido.focus();
          return false;
     }
/*
     if(frm.gugun.selectedIndex == 0)
     {
          alert("구/군 선택을 해주세요");
          frm.gugun.focus();
          return false;
     }
*/
     return true;
}
</script>
