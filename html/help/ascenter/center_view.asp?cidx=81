<script language='javascript'>
function print(path)
{
     //window.print();
}
function _del(ccidx,writer)
{
     if(writer != "")
     {
          alert("작성자만 삭제가 가능합니다.");
     }
     else
     {
          location.href="center_comment_save.asp?ccidx=" + ccidx + "&writer="+writer+"&act=delete&cidx=81"
     }

}
function comment_str(frm)
{
     if(frm.comment.value == "")
     {
          alert("코멘트를 입력해 주세요~");
          frm.comment.focus();
          return false
     }

     return true;
}

function search(frm)
{
     if(frm.sword.value=="")
     {
          alert("검색어를 입력하세요~");
          frm.sword.focus();
          return false;
     }

     return true;
}
</script>
