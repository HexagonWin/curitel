	function goDel(idx,uid){
		var df = document.reviewForm;	
		
		if (uid == ""){
			df.pro.value = "del"
			df.r_idx.value = idx
			df.target = "review_proc"
			df.action = "review_proc.asp"
			df.submit();
		}else{
			alert("본인이 작성한 글만 삭제 할 수 있습니다.");
			return;
		}
	}

</script>
