	function goWish(pro){
		df = document.procForm;
		
				confirmLogin();			
		
	}
	function goList(service,page){
		var nUrl;
		if(service=="SKT")
			nUrl = 'skt.asp';
		if(service=="KTF")
				nUrl = 'ktf.asp';
		if(service=="LGT")
				nUrl = 'lgt.asp';
		if(service=="")
				nUrl = 'index.asp';
		nUrl = nUrl + '?page='+page;
		location.href=nUrl;
	}
</script>
