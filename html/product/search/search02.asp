	var chk_c_list = "";	// 삭제 대상이 될 데이터 리스트

	// check box 처리
	function setGValue(bmode, chb_no){
		var df = document.ins_form;
		var chkbox = eval("df.chk_"+chb_no);

		if(bmode){				
			if(!chkSome(chkbox.value)){
				chk_c_list += chkbox.value+";";
			}
		}else{
			if(chkSome(chkbox.value)){
				del(chkbox.value);
			}
		}
//		alert(chk_c_list);
	}
	//  가지고 있는 카테고리 리스트에 받은 데이터가 있는 지 검사
	function chkSome(chkVal){
		var result = false;
		var space = ";";
		var split_goods_list = chk_c_list.split(space);
		for(var i=0; i<split_goods_list.length; i++){
			if(split_goods_list[i] == chkVal) result = true;
		}
		return result;
	}
	// 카테고리 목록에서 데이터 삭제
	function del(chkVal){
		var space = ";";
		var split_goods_list = chk_c_list.split(space);
		for(var i=0; i<split_goods_list.length; i++){
			if(split_goods_list[i] == chkVal){
				chk_c_list = chk_c_list.replace(chkVal+";", "");
			}
		}
	}

	function searchCate(){		
		var df = document.ins_form;

		//check box 처리
		df.c_list.value = chk_c_list;
		df.action = "search02_result.asp";

		if(!df.c_list.value){
			alert("Step1 혹은 Step2의 검색 항목을 선택 하셔야 합니다.");
			return;
		}

		df.submit();

	}
