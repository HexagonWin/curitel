	function comm_prcosess(){
		if(document.comment.comment.value == ""){
			alert("한줄 리플 내용을 입력해주세요!");
			document.comment.comment.focus();
			return false;
		}
	}

	function del(div_seq,cidx){
		if(confirm("정말로 삭제하시겠습니까?")){
			document.location = "/html/knowledge/comment_process.asp?prc_seq=D&div_seq="+div_seq+"&pidx=537&cidx="+cidx
		}
	}
//-->
</SCRIPT>

