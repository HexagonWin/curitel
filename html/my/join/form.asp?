function foreignMemberJoin()
{
	document.all.normalMember1.style.display = "none";
	document.all.normalMember2.style.display = "none";
	document.all.normalMember3.style.display = "none";
	document.all.foreignMember1.style.display = "block";
	document.all.foreignMember2.style.display = "block";
	document.all.foreignMember3.style.display = "block";
	document.myform.jmode.value = "F";
}
function normalMemberJoin()
{
	document.all.normalMember1.style.display = "block";
	document.all.normalMember2.style.display = "block";
	document.all.normalMember3.style.display = "block";
	document.all.foreignMember1.style.display = "none";
	document.all.foreignMember2.style.display = "none";
	document.all.foreignMember3.style.display = "none";
	document.myform.jmode.value = "N";
}

function eng_ck(obj)
{
	var ck_ok = "true";
	var str_check = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789";		
	for (i=0;i<obj.length; i++){
		idcheck = obj.charAt(i);
		for (j=0;j<str_check.length;j++) if (idcheck == str_check.charAt(j)) break;
		if (j == str_check.length) {						
			ck_ok = "false";		
			return ck_ok ;
		}
	}
}
function num_ck(obj)
{
	var ck_ok = "true";
	var str_check = "0123456789";		
	for (i=0;i<obj.length; i++){
		idcheck = obj.charAt(i);
		for (j=0;j<str_check.length;j++) if (idcheck == str_check.charAt(j)) break;
		if (j == str_check.length) {						
			ck_ok = "false";		
			return ck_ok ;
		}
	}
}
function IsValidSocNumber(n)
{
	var snu,k,c,remainder,check_sum;
	var sn = new String(n);    
   check_num=sn.substr(12,1);
   sum=0;
   k=0;

   for (var i=0; i<12; i++) {
		j = sn.substr(i, 1);
		c = i + 2;
		if (c > 9) { c = c - 8; }
	k = c * j;
	sum = sum + k;
   }

   remainder = sum % 11;
   check_sum = 11 - remainder;
   if (check_sum > 9) {
	   check_sum = check_sum - 10;
   }

   if (check_num == check_sum) return true;
   else return false;
}

function IsValidSocNumber(n)
{
	var snu,k,c,remainder,check_sum;
	var sn = new String(n);    
   check_num=sn.substr(12,1);
   sum=0;
   k=0;

   for (var i=0; i<12; i++) {
		j = sn.substr(i, 1);
		c = i + 2;
		if (c > 9) { c = c - 8; }
	k = c * j;
	sum = sum + k;
   }

   remainder = sum % 11;
   check_sum = 11 - remainder;
   if (check_sum > 9) {
	   check_sum = check_sum - 10;
   }

   if (check_num == check_sum) return true;
   else return false;
}

function checkid(obj)
{
	var str;
	str = document.myform.uid.value;
	if (!str) {
		alert ("사용하실 아이디를 입력해주세요.");
		selectAll(document.myform.uid);
		return false;
	}
	else if(str.length < 4) {
		alert("아이디는 최소한 4자이상 입니다.");
		selectAll(document.myform.uid);
		return false;
	}
	else if(str=="admin"||str=="master"||str=="webmaster"){
		alert("사용할 수 없는 아이디 입니다.");
		selectAll(document.myform.uid);
		return false;		
	}
	ck_ok = eng_ck(str);
	if ( ck_ok == "false"){
		alert("ID는 영문자와 숫자만으로 적어주세요.");
		selectAll(document.myform.uid);		
		return false;				
	}else{
		if (obj == "ck"){
		window.open("checkid.asp?uid="+str, "checkid", "scrollbars=no, resizeable=no,width=400, height=200");
		}
	}			
}	

function selectAll(obj)
{
		if (obj.type!='hidden'&&obj.style.display!='none') {
			obj.focus();
			if (!obj.readOnly) obj.select();
		}
}

function oldmember()
{
	var str;
	str = document.myform.uid.value;
	if (!str) {
		alert ("사용하실 아이디를 입력해주세요.");
		selectAll(document.myform.uid);
		return false;
	}
	else if(str.length < 4) {
		alert("아이디는 최소한 4자이상 입니다.");
		selectAll(document.myform.uid);
		return false;
	}
	else if(str=="admin"||str=="master"||str=="webmaster"){
		alert("사용할 수 없는 아이디 입니다.");
		selectAll(document.myform.uid);
		return false;		
	}
	window.open("checkid.asp?mode=oldid&uid="+str, "checkid", "scrollbars=no, resizeable=no,width=400, height=200");			
}

function zipcodesearch()
{
document.domain = "curitel.com";	window.open("/common/zipcode.asp?frmName=myform&zipcode1=zipcode1&zipcode2=zipcode2&address1=address1&address2=address1","POST","width=480,height=300,marginwidth=0,marginheight=0,resizable=0,scrollbars=yes");
	return;	   
}

function submitto()
{	
	var form = document.myform		
	str = document.myform.uid.value;
	if (!str) {
		alert ("사용하실 아이디를 입력해주세요.");
		selectAll(document.myform.uid);
		return false;
	}
	else if(str.length < 4) {
		alert("아이디는 최소한 4자이상 입니다.");
		selectAll(document.myform.uid);
		return false;
	}
	else if(str=="admin"||str=="master"||str=="webmaster"){
		alert("사용할 수 없는 아이디 입니다.");
		selectAll(document.myform.uid);
		return false;		
	}
	ck_ok = eng_ck(str);
	if ( ck_ok == "false"){
		alert("ID는 영문자와 숫자만으로 적어주세요.");
		selectAll(document.myform.uid);		
		return false;	
	}	
	
	if(form.jmode.value == "N")
	{
		if (!form.name.value) {
			alert ("이름을 확인해주세요\n이름은 필수사항이며 한글외에는 들어갈수 없습니다.");
			selectAll(document.myform.name);
			return false;
		}else {
				for(i=0;i<form.name.value.length;i++) { 
					var a=form.name.value.charCodeAt(i); 
					if (a < 128) { 
						alert ("이름을 확인해주세요\n이름은 필수사항이며 한글외에는 들어갈수 없습니다.");
						form.name.value=""; 
						form.name.focus(); 
						return false;
					} 
				}
		}		
	} else {
		if (!form.name.value) {
			alert ("이름을 입력하세요.");
			selectAll(document.myform.name);
			return false;
		}
	}

	if(form.jmode.value == "N")
	{
		if(form.ssn1.value.length < 6 || form.ssn2.value.length < 7 ){
			alert("주민등록 번호를 입력해 주세요.");
			selectAll(document.myform.ssn1);
			return false;
		}
		var SN = form.ssn1.value+form.ssn2.value;
		if (!IsValidSocNumber(SN)) {
			alert("주민등록번호를 정확하게 입력해 주세요.");
			selectAll(document.myform.ssn1);
			return false;
		}
	} else {
		if(form.passportno.value == "")
		{
			alert("Passport No.를 입력하세요.");
			form.passportno.foscus();
			return false;
		}
	}

	if(!form.passwd.value || !form.passwd2.value){
		alert("비밀번호를 입력하세요.");
		selectAll(document.myform.passwd);
		return false;
	}

	if(form.passwd.value != form.passwd2.value){
		alert("비빌번호가 일치하지 않습니다.");
		document.myform.passwd.value = "";
		document.myform.passwd2.value = "";
		selectAll(document.myform.passwd);
		return false;
	}

	if(!form.zipcode1.value || !form.zipcode2.value || !form.address1.value){
	  alert("주소를 입력하세요.");
	  return false;
  }	
/*
	if(!form.address2.value){
	  alert("주소를 입력하세요.");
		selectAll(document.myform.address2);
	  return false;
  }	
*/
	if(!form.tel1.value || !form.tel2.value || !form.tel3.value){
	  alert("전화번호를 확인해주세요.");
	  selectAll(document.myform.tel1);
	  return false;
  }
	if ( !isNumber(form.tel2) ){
		alert("전화번호는 숫자형식으로 입력해 주십시오.");
		form.tel2.value = "";
		form.tel2.focus();
		return false;
	}else if ( form.tel2.value.length < 3 ){
		alert("올바른 전화번호 형식(국번길이)이 아닙니다.확인해 주시기 바랍니다.");
		form.tel2.focus();
		return false;
	}
	if ( !isNumber(form.tel3) ){
		alert("전화번호는 숫자형식으로 입력해 주십시오.");
		form.tel3.value = "";
		form.tel3.focus();
		return false;
	}else if ( form.tel3.value.length < 4 ){
		alert("올바른 전화번호 형식이 아닙니다.확인해 주시기 바랍니다.");
		form.tel3.focus();
		return false;
	}

	if(!form.mobile2.value || !form.mobile3.value){
	  alert("핸드폰번호를 확인해주세요.");
	  selectAll(document.myform.mobile2);
	  return false;
  }
	if ( !isNumber(form.mobile2) ){
		alert("전화번호는 숫자형식으로 입력해 주십시오.");
		form.mobile2.value = "";
		form.mobile2.focus();
		return false;
	}else if ( form.mobile2.value.length < 3 ){
		alert("올바른 전화번호 형식(국번길이)이 아닙니다.확인해 주시기 바랍니다.");
		form.mobile2.focus();
		return false;
	}
	if ( !isNumber(form.mobile3) ){
		alert("전화번호는 숫자형식으로 입력해 주십시오.");
		form.mobile3.value = "";
		form.mobile3.focus();
		return false;
	}else if ( form.mobile3.value.length < 4 ){
		alert("올바른 전화번호 형식이 아닙니다.확인해 주시기 바랍니다.");
		form.mobile3.focus();
		return false;
	}

	if (!form.email.value){
	  alert("E-mail을 확인해주세요.");
	  selectAll(document.myform.email);
	  return false;
  } else {
		if ( !isValidEmail( form.email) ){
				alert("올바른 이메일 형식이 아닙니다. 확인하시고 다시 입력해 주시기 바랍니다.");
				selectAll(document.myform.email);
				return false;
			}
/*			
		if (form.email.value.indexOf("@hanmail.net") > -1 || form.email.value.indexOf("@daum.net") >-1) {
			alert ("당사는 e-Mail로 고객에 대해 각종 정보를 제공하고 있으나, \n"+
				   "입력하신 '다음' 계정(@hanmail.net, @daum.net)은 \n"+
				   "'다음'의 온라인 우표제 시행으로 회원님에게 정보를 \n"+
				   "원활하게 제공할 수 없어 등록을 받지 않습니다.\n"+
				   "회원등록시 다른 메일계정으로 등록을 하여 주십시오");
			form.email.value = "";
			selectAll(document.myform.email);
			return false;			
		}
*/		
	}

	if (form.uid.value == form.recomm_id.value){
		alert("자신을 추천 할수는 없습니다.");
		form.recomm_id.value = "";
		selectAll(document.myform.recomm_id);
		return false;
	}
}
function isValidEmail(input) {
//    var format = /^(\S+)@(\S+)\.([A-Za-z]+)$/;
    var format = /^((\w|[\-\.])+)@((\w|[\-\.])+)\.([A-Za-z]+)$/;
    return isValidFormat(input,format);
}
function isValidFormat(input,format) {
    if (input.value.search(format) != -1) {
        return true; //올바른 포맷 형식
    }
    return false;
}
function isNumber(input) {
    var chars = "0123456789";
    return containsCharsOnly(input,chars);
}
function containsCharsOnly(input,chars) {
    for (var inx = 0; inx < input.value.length; inx++) {
       if (chars.indexOf(input.value.charAt(inx)) == -1)
           return false;
    }
    return true;
}

